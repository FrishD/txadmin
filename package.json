{
    "name": "txadmin",
    "version": "1.0.0",
    "description": "The official FiveM/RedM server management platform used by tens of thousands of servers.",
    "module": "./core/index.js",
    "type": "module",
    "workspaces": [
        "core",
        "nui",
        "panel",
        "shared"
    ],
    "scripts": {
        "build": "rimraf dist tmp_core_tsc && npm run build -w nui && npm run build -w panel && npm run build -w core && (npm run license:distfile || true)",
        "prepare": "husky",
        "test": "npm run test --workspaces",
        "typecheck": "npm run typecheck --workspaces",
        "locale:rebase": "node scripts/locale-utils.js rebase",
        "locale:check": "node scripts/locale-utils.js check --color",
        "license:report": "rimraf .reports/license && mkdir \".reports/license\" && npm run license:report --workspaces",
        "license:list": "node scripts/list-licenses.js",
        "license:distfile": "npx generate-license-file --ci --no-spinner --input package.json --output dist/THIRD-PARTY-LICENSES.txt"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tabarra/txAdmin.git"
    },
    "author": "Andr√© Tabarra",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tabarra/txAdmin/issues"
    },
    "homepage": "https://txadmin.gg/",
    "optionalDependencies": {
        "@swc/core-win32-x64-msvc": "1.15.7",
        "@swc/core-win32-ia32-msvc": "1.15.7",
        "@swc/core-win32-arm64-msvc": "1.15.7",
        "@swc/core-linux-x64-gnu": "1.15.7",
        "@swc/core-linux-x64-musl": "1.15.7",
        "@swc/core-linux-arm64-gnu": "1.15.7",
        "@swc/core-linux-arm64-musl": "1.15.7",
        "@swc/core-darwin-x64": "1.15.7",
        "@swc/core-darwin-arm64": "1.15.7"
    },
    "dependencies": {
        "@mui/icons-material": "^6.5.0",
        "@mui/material": "^6.5.0",
        "@mui/styled-engine": "^7.3.6",
        "@mui/system": "^7.3.6",
        "@mui/utils": "^7.3.6",
        "chalk": "^5.3.0",
        "dequal": "^2.0.3",
        "humanize-duration": "^3.32.1",
        "jsonrepair": "^3.11.2",
        "lodash-es": "^4.17.21",
        "semver": "^7.6.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@types/humanize-duration": "^3.27.4",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.7.5",
        "esbuild": "~0.24.0",
        "eslint": "^9.12.0",
        "generate-license-file": "^3.5.1",
        "husky": "^8.0.3",
        "rimraf": "^6.0.1",
        "rollup-plugin-visualizer": "^5.12.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "utility-types": "^3.11.0",
        "vitest": "^2.1.9"
    }
}
